<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="ImagesDAO" >

  	<!-- 제품 등록 -->
	<insert id="sellerInsertImage">
  		insert into images(ino,pno,main_img1,main_img2,main_img3,main_img4,main_img5,
  		con_img_1,con_img_2)
  		values(seq_images.nextval,#{pno},#{main_img1},#{main_img2},#{main_img3},#{main_img4},#{main_img5},
  		#{con_img_1},#{con_img_2})
  	</insert>
  
  	
  	
  </mapper>